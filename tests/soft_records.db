  field(DRVL, "10")
  field(LOW, "5")
  field(LOLO, "2")
  field(LOPR, "0")
  field(VAL, "$(VAL=42)")
  field(PINI, "YES")
  field(HHSV, "MAJOR")
  field(HSV, "MINOR")
